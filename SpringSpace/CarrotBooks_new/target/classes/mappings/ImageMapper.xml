<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
			
<mapper namespace="ImageMapper">
	<select id="getAttachList" resultType="attachImageVO">
		select * from advertImage where advertId = #{refId}
	</select>

	<select id="getAttachAll" resultType="attachImageVO">
		select * from advertImage
	</select>

</mapper>